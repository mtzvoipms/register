.row.raw_data_record{ id: "raw_data_record_#{raw_record.data.etag}" }
  .col-lg-8
    .raw-data-records
      %pre.pre-scrollable= JSON.pretty_generate(raw_record.data.to_h)
  .col-lg-4
    .frame-light.meta
      .frame-wrap
        %h6= t("raw_data_records.show.data_sources")
        %p= true
        %h6= t("raw_data_records.show.seen_in_most_recent_import")
        %p= true
        %h6= t("raw_data_records.show.last_seen")
        %p= true
        %h6= t("raw_data_records.show.first_seen")
        %p= true
        %h6= t("raw_data_records.show.fields.etag")
        %p
          %code= raw_record.data.etag
